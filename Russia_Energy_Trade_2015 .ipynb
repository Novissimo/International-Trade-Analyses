    "def autoType(code):\n",
    "    if code == '271111':\n",
    "        return 'Liquid Natural Gas'\n",
    "    if code == '271121':\n",
    "        return 'Natural Gas (gaseous state)'\n",
    "    if code == '3811':\n",
    "        return 'Gasoline'\n",
    "    if code == '2709':\n",
    "        return 'Petroleum Oils'\n",
    "    else:\n",
    "        return 'Unkown'\n",
    "    \n",
    "COMMODITY = 'Commodity'\n",
    "energy[COMMODITY] = energy['Commodity Code'].apply(autoType)\n",
    "MONTH = 'Period'\n",
    "PARTNER = 'Partner'\n",
    "FLOW = 'Trade Flow'\n",
    "VALUE = 'Trade Value (US$)'\n",
    "headings = [MONTH, PARTNER, FLOW, COMMODITY, VALUE]\n",
    "energy = energy[headings]\n",
    "energy.head(10)"
   ]
